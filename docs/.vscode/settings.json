{
  "deno.enable": true,
  "deno.unstable": true,
  "deno.config": "./deno.json",
  "deno.importMap": "./import_map.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": false
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
}
